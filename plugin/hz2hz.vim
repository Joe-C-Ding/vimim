" ===============================================================
"                       VIM Logic Control
" ===============================================================
if exists("loaded_chinese_transfer") || &cp || &encoding != "utf-8"
   finish
end
let loaded_chinese_transfer = 1

let s:datafile_loaded=0
let s:traditional_chinese="big5"
let s:simplified_chinese="gb"

" Global variables:
"
" Simplified Chinese is set by default,
" therefore, :ChineseToggle will do gS, for the first time
"
" To make :ChineseToggle will do gT for the first time,
" define the following in the ~/.vimrc file:
"   let g:ChineseToggle = 1

" vn gS       :GetSimplifiedChinese<CR>
" vn gT       :GetTraditionalChinese<CR>
" nn gS  mzHmt:GetSimplifiedChinese<CR>`tzt`z
" nn gT  mzHmt:GetTraditionalChinese<CR>`tzt`z

com! -range=% GetSimplifiedChinese  <line1>,<line2>call GetSimplifiedChinese()
com! -range=% GetTraditionalChinese <line1>,<line2>call GetTraditionalChinese()
com! -range=% ChineseToggle         <line1>,<line2>call ChineseToggle()

if !exists('g:ChineseToggle')
    let g:ChineseToggle = 0
endif

" Following functions will take care of range.
" Following functions will abort as soon as an error is detected.
"
function! ChineseToggle() range abort
  let g:ChineseToggle = g:ChineseToggle+1
  if(g:ChineseToggle%2)
    exe a:firstline.",".a:lastline.'GetSimplifiedChinese'
  else
    exe a:firstline.",".a:lastline.'GetTraditionalChinese'
  endif
endfunction

function! GetSimplifiedChinese() range abort
  call s:Load_Datafile()
  let from = s:traditional_chinese
  let to   = s:simplified_chinese
  exe a:firstline.",".a:lastline.'call Transfer(from, to)'
endfunction

function! GetTraditionalChinese() range abort
  call s:Load_Datafile()
  let from = s:simplified_chinese
  let to   = s:traditional_chinese
  exe a:firstline.",".a:lastline.'call Transfer(from, to)'
endfunction

function! Transfer(from, to) range abort
  let from = a:from
  let to   = a:to
  let s:gdefault_save = &gdefault
  set gdefault
  exe a:firstline.",".a:lastline.'s/./\=s:OneToOneMap(submatch(0),from,to)'
  let &gdefault = s:gdefault_save
  unlet s:gdefault_save
endfunction


function! s:OneToOneMap(char, from, to)
  if char2nr(a:char) < 19968 || char2nr(a:char) > 40869
      return a:char
  endif
  let i = match(a:from, a:char)
  return (i >= 0 ? strpart(a:to, i, 3) : a:char)
endfunction
